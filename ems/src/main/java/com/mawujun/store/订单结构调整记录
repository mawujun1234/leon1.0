create table ems_order_0516 as select * from ems_order

drop table ems_order

select * from ems_order_0516

insert into ems_order(id,orderNo,store_id,orderDate,operater,status,project_id,createDate) 
select sys_guid(),orderNo,store_id,orderDate,operater,status,project_id,createDate from (
select distinct orderNo,store_id,orderDate,operater,status,project_id,createDate from ems_order_0516
)


insert into ems_orderlist(id,order_id,type_id,subtype_id,prod_id,brand_id,supplier_id,style,unitPrice,orderNum,totalNum) 
select sys_guid(),id,type_id,subtype_id,prod_id,brand_id,supplier_id,style,unitPrice,orderNum,totalNum from (
select distinct b.id,type_id,subtype_id,prod_id,brand_id,supplier_id,style,unitPrice,orderNum,totalNum from ems_order_0516 a,ems_order b
where a.orderNo=b.orderNo
)

删除 EMS_BARCODE的ORDER_ID列
删除 ems_equipment的ORDER_ID列
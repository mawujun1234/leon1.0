<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!-- mawujun qq:16064988 e-mail:16064988@qq.com-->
<mapper namespace="com.mawujun.check.TrimRepository">
	<!-- 查询语句，会自动分页-->
	<sql id="queryPage_where">
		<where>
		
		</where>
	</sql>
    <select id="queryPage" resultType="com.mawujun.check.Trim" parameterType="map">
    	select * from ems_trim a
    	<include refid="queryPage_where" />
    </select> 
     <!-- 名称模式为：****_count,也可以不写，但如果查询叫复杂的话，自己写有助于控制查询性能-->
    <select id="queryPage_count" resultType="int" parameterType="map">
    	select count(a.*)
		from ems_trim a
    </select>
</mapper>

